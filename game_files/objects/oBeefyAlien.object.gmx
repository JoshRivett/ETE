          <argument>
            <kind>1</kind>
            <string>//Exits the script if paused
if global.paused = true {
    image_speed = 0;
    exit;
} else {
    image_speed = 10 / room_speed;
}

//Look Direction
if instance_exists(oPlayer) and playerSeen = true {
    playerDir = point_direction(x, y, oPlayer.x, oPlayer.y);

    if playerDir &gt;= 270 or playerDir &lt;= 90 {
        sprite_index = rightSprite;
        //hsp = 1;
    } else if playerDir &gt; 90 and playerDir &lt; 270 {
        sprite_index = leftSprite;
        //hsp = -1;
    }
}

//Gravity
vsp += grav;

//Horizontal Collision
if place_meeting(x + hsp, y, oWall) {
    while not(place_meeting(x + sign(hsp), y, oWall)) {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

//Vertical Collision
if place_meeting(x, y + vsp, oWall) {
    while not(place_meeting(x, y + sign(vsp), oWall)) {
        y += sign(vsp);
    }
    vsp = 0;
    movesp = movementSpeed;
}
y += vsp;

//Pixel Grid
x = round(x);
y = round(y);

//Once player has been seen
if playerSeen = true {
    //Alarm Tick
    timeSinceShot += 1;
    
    if instance_exists(oPlayer) {
        if distance_to_object(oPlayer) &gt; 175 {
            if playerDir &gt;= 270 or playerDir &lt;= 90 {
                hsp = movesp;
                rightSprite = sBeefyAlienRW;
            } else if playerDir &gt; 90 and playerDir &lt; 270 {
                hsp = -movesp;
                leftSprite = sBeefyAlienLW;
            }
        } else {
            hsp = 0;
            leftSprite = sBeefyAlienL;
            rightSprite = sBeefyAlienR;
        }
    }
}

//Shot Cooldown Alarm
if timeSinceShot &gt;= shotcd {
    if instance_exists(oPlayer) {
        if playerDir &gt;= 270 or playerDir &lt;= 90 {
            plasma = instance_create(x + 10, y + 1, oPlasmaShot);
            plasmaDir = 0;
        } else if playerDir &gt; 90 and playerDir &lt; 270 {
            plasma = instance_create(x - 10, y + 1, oPlasmaShot);
            plasmaDir = 180;
        }
        
        plasma.sp = plasmaSp;
        plasma.direction = plasmaDir;
                
        timeSinceShot = 0;
    }
}
</string>
