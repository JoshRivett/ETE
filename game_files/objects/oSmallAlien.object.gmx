            <string>//Exits the script if paused
if global.paused = true {
    exit;
}

//Look Direction

if instance_exists(oPlayer) {
    playerDir = point_direction(x, y, oPlayer.x, oPlayer.y);

    if playerDir &gt; 270 or playerDir &lt; 90 {
        image_index = 0;
        hsp = 1;
    } else if playerDir &gt; 90 and playerDir &lt; 270 {
        image_index = 1;
        hsp = -1;
    }
}

//Gravity
vsp += grav;

//Horizontal Collision
if place_meeting(x + hsp, y, oWall) {
    while not(place_meeting(x + sign(hsp), y, oWall)) {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

//Vertical Collision
if place_meeting(x, y + vsp, oWall) {
    while not(place_meeting(x, y + sign(vsp), oWall)) {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

//Pixel Grid
x = round(x);
y = round(y);
